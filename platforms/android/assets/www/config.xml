<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Akruti.Android" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
<name>My Nursery Rhymes</name>
    <description>
        Simple and free Nursery Rhymes video app
    </description>
    <author email="khatri.hitesh@gmail.com" href="http://khatri.hitesh.com">
        Hitesh Khatri
    </author>
    <content src="index.html" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>    
    <access origin="*" />
    <access origin="http://youtube.com" subdomains="true" />
    <preference name="useBrowserHistory" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
